{
  "colorschemes": {
    "body": [
      "[[plugins.colorschemes]]",
      "name = '$1'"
    ],
    "description": "colorscheme plugin definition",
    "prefix": [
      "color",
      "colorschemes"
    ]
  },
  "ftplugin": {
    "body": [
      "[plugins.ftplugin]",
      "${1:filetype} = '''",
      "$0",
      "'''"
    ],
    "description": "dein ftplugin definition",
    "prefix": [
      "ftplugin"
    ]
  },
  "hook": {
    "body": [
      "hook_${1:add} = '''",
      "$0",
      "'''"
    ],
    "description": "dein hook definition",
    "prefix": [
      "hook"
    ]
  },
  "on": {
    "body": [
      "on_${1:cmd} = ['$0']"
    ],
    "description": "dein on_* option definition",
    "prefix": [
      "on"
    ]
  },
  "plugins": {
    "body": [
      "[[plugins]]",
      "repo = '$1'"
    ],
    "description": "dein plugin definition",
    "prefix": [
      "plugins"
    ]
  }
}
