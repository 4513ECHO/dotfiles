{
  "*": {
    "config": {
      "completion_item_kinds": {
        "1": "Text",
        "2": "Method",
        "3": "Func",
        "4": "Construct",
        "5": "Field",
        "6": "Var",
        "7": "Class",
        "8": "Interf",
        "9": "Module",
        "10": "Prop",
        "11": "Unit",
        "12": "Value",
        "13": "Enum",
        "14": "Keyword",
        "15": "Snip",
        "16": "Color",
        "17": "File",
        "18": "Refer",
        "19": "Folder",
        "20": "Member",
        "21": "Const",
        "22": "Struct",
        "23": "Event",
        "24": "Operat",
        "25": "Param"
      },
      "symbol_kinds": {
        "1": "File",
        "2": "Module",
        "3": "NameSpace",
        "4": "Package",
        "5": "Class",
        "6": "Method",
        "7": "Property",
        "8": "Field",
        "9": "Constructor",
        "10": "Enum",
        "11": "Interface",
        "12": "Function",
        "13": "Variable",
        "14": "Constant",
        "15": "String",
        "16": "Number",
        "17": "Boolean",
        "18": "Array",
        "19": "Object",
        "20": "Key",
        "21": "Null",
        "22": "Member",
        "23": "Structure",
        "24": "Event",
        "25": "Operator",
        "26": "Parameter"
      }
    }
  },
  "bash-language-server": {
    "disabled": true
  },
  "deno": {
    "initialization_options": {
      "enable": true,
      "lint": true,
      "suggest": {
        "autoImports": false,
        "hosts": {
          "https://crux.land": true,
          "https://deno.land": true,
          "https://x.nest.land": true
        }
      },
      "unstable": true
    }
  },
  "efm-langserver": {
    "allowlist": [
      "json",
      "lua",
      "markdown",
      "sh",
      "yaml"
    ],
    "disabled": false
  },
  "pylsp-all": {
    "cmd": [
      "pylsp"
    ],
    "workspace_config": {
      "pylsp": {
        "configurationSources": [
          "flake8"
        ],
        "plugins": {
          "black": {
            "enabled": true
          },
          "flake8": {
            "enabled": true
          },
          "mccabe": {
            "enabled": false
          },
          "pycodestyle": {
            "enabled": false
          },
          "pyflakes": {
            "enabled": false
          },
          "pyls_isort": {
            "enabled": true
          },
          "pylsp_mypy": {
            "enabled": true,
            "overrides": [
              "--no-pretty",
              true
            ],
            "report_progress": true
          }
        }
      }
    }
  },
  "sumneko-lua-language-server": {
    "cmd": [
      "lua-language-server"
    ],
    "workspace_config": {
      "Lua": {
        "completion": {
          "showWord": "Disable"
        },
        "diagnostics": {
          "disable": [
            "lowercase-global"
          ],
          "enable": true,
          "globals": [
            "packer_plugins",
            "vim"
          ]
        },
        "runtime": {
          "path": [
            "?.lua",
            "?/init.lua",
            "?/?.lua"
          ],
          "version": "LuaJIT"
        },
        "telemetry": {
          "enable": false
        },
        "workspace": {
          "checkThirdParty": false,
          "library": [],
          "maxPreload": 1000
        }
      }
    }
  },
  "taplo-lsp": {
    "cmd": [
      "taplo",
      "lsp",
      "stdio"
    ],
    "workspace_config": {
      "evenBetterToml": {
        "schema": {
          "associations": {
            "/dein/.*\\.toml": "This will replaced in vim script"
          }
        }
      }
    }
  },
  "vim-language-server": {
    "disabled": true
  },
  "vscode-json-language-server": {
    "cmd": [
      "deno",
      "run",
      "--allow-all",
      "--no-config",
      "--no-lock",
      "--node-modules-dir=false",
      "npm:vscode-langservers-extracted@4.7.0/vscode-json-language-server",
      "--stdio"
    ]
  },
  "yaml-language-server": {
    "workspace_config": {
      "yaml": {
        "completion": true,
        "hover": true,
        "schemas": {
          "https://mattn.github.io/efm-langserver/schema.json": [
            "/efm-langserver/config.yaml"
          ],
          "https://pax.deno.dev/aquaproj/aqua@v2.11.0-4/json-schema/aqua-yaml.json": [
            "/aqua/aqua.yaml"
          ],
          "https://pax.deno.dev/aquaproj/aqua@v2.11.0-4/json-schema/registry.json": [
            "/aqua-registry/pkgs/**/registry.yaml",
            "/aqua-registry/registry.yaml",
            "/aqua/experimental.yaml",
            "/aqua/registry.yaml"
          ]
        },
        "validate": true
      }
    }
  }
}
