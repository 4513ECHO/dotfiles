export PATH="$HOME/.local/bin:$PATH"
export DOTPATH="$HOME/dotfiles"

export ZDOTDIR="$HOME"

export EDITOR="vim"
export PAGER="less"
export LESS="-R"
export LANG="ja_JP.UTF-8"

export HISTFILE="$ZDOTDIR/.zsh_history"
export HISTSIZE=1000000
export SAVEHIST=1000000

export WORDCHARS='*?_-.[]~=&;!#$%^(){}<>'
