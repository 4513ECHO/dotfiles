{
  "automerge": true,
  "branchConcurrentLimit": 0,
  "dependencyDashboard": true,
  "extends": [
    "config:base",
    "github>aquaproj/aqua-renovate-config#1.2.5",
    "github>aquaproj/aqua-renovate-config:file#1.2.5(config/aqua/aqua\\.yaml)"
  ],
  "labels": [
    "dependencies"
  ],
  "platformAutomerge": true,
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "regexManagers": [
    {
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "actions/go-versions",
      "fileMatch": [
        ".github/workflows/aqua.yaml"
      ],
      "matchStrings": [
        "go-version: \"(?<currentValue>.*?)\""
      ]
    },
    {
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "denoland/deno",
      "fileMatch": [
        ".github/workflows/udd.yaml"
      ],
      "matchStrings": [
        "deno-version: (?<currentValue>.*?)"
      ]
    }
  ]
}
