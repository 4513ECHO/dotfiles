{
  "automerge": true,
  "branchConcurrentLimit": 0,
  "dependencyDashboard": true,
  "extends": [
    "config:base",
    "github>aquaproj/aqua-renovate-config#2.3.1",
    "github>aquaproj/aqua-renovate-config:file#2.3.1(config/aqua/aqua\\.yaml)",
    "github>aquaproj/aqua-renovate-config:installer-script#2.3.1(Makefile)"
  ],
  "labels": [
    "dependencies"
  ],
  "platformAutomerge": true,
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "customManagers": [
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "actions/go-versions",
      "fileMatch": [
        "\\.github/workflows/aqua\\.yaml"
      ],
      "matchStrings": [
        "go-version: \"(?<currentValue>.*?)\""
      ]
    }
  ]
}
